<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="default" namespace="/" extends="struts-default">
        <action name="test" class="com.lt.action.Test">
            <result name="success">/main.jsp</result>
            <result name="false">/false.jsp</result>
        </action>
        <action name="testImpAction" class="com.lt.action.TestImpAction">
            <result name="success">/main.jsp</result>
            <result name="error">/false.jsp</result>
        </action>
        <action name="testExtendsActionSupport" class="com.lt.action.TestExtendsActionSupport">
            <result name="success">/main.jsp</result>
            <result name="error">/false.jsp</result>
        </action>
    </package>
</struts>
